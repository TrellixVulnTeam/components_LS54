#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/typescript@4.2.4/node_modules/typescript/bin/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/typescript@4.2.4/node_modules/typescript/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/typescript@4.2.4/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/node_modules:/Users/adamjosefus/Documents/Projekty/node_modules:/Users/adamjosefus/Documents/node_modules:/Users/adamjosefus/node_modules:/Users/node_modules:/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/@snowpack+plugin-typescript@1.2.1_typescript@4.2.4/node_modules/@snowpack/plugin-typescript/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/@snowpack+plugin-typescript@1.2.1_typescript@4.2.4/node_modules/@snowpack/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/@snowpack+plugin-typescript@1.2.1_typescript@4.2.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typescript@4.2.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../../typescript@4.2.4/node_modules/typescript/bin/tsserver" "$@"
fi
