#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/pacote@11.3.3/node_modules/pacote/lib/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/pacote@11.3.3/node_modules/pacote/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/pacote@11.3.3/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/node_modules:/Users/adamjosefus/Documents/Projekty/node_modules:/Users/adamjosefus/Documents/node_modules:/Users/adamjosefus/node_modules:/Users/node_modules:/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules/pacote/lib/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules/pacote/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pacote/lib/bin.js" "$@"
else
  exec node  "$basedir/../pacote/lib/bin.js" "$@"
fi
