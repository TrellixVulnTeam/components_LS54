#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/esbuild@0.9.7/node_modules/esbuild/bin/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/esbuild@0.9.7/node_modules/esbuild/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/esbuild@0.9.7/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/node_modules:/Users/adamjosefus/Documents/Projekty/node_modules:/Users/adamjosefus/Documents/node_modules:/Users/adamjosefus/node_modules:/Users/node_modules:/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules/esbuild/bin/node_modules:/Users/adamjosefus/Documents/Projekty/Templatone/repos/components/node_modules/.pnpm/node_modules/esbuild/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
